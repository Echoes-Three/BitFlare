<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="SoftLatte">#DCD2C8</Color>
    <Color x:Key="DarkLatte">#B2A799</Color>
    <Color x:Key="WalnutBrown">#594D41</Color>
    <Color x:Key="PoppyRed">#D63640</Color>
    
    <SolidColorBrush x:Key="SoftLatteBrush" Color="{StaticResource SoftLatte}" />
    <SolidColorBrush x:Key="DarkLatteBrush" Color="{StaticResource DarkLatte}" />
    <SolidColorBrush x:Key="WalnutBrownBrush" Color="{StaticResource WalnutBrown}" />
    <SolidColorBrush x:Key="PoppyRedBrush" Color="{StaticResource PoppyRed}" />
    <SolidColorBrush x:Key="WhiteBrush" Color="White" />
    <SolidColorBrush x:Key="BlackBrush" Color="Black" />
</ResourceDictionary>